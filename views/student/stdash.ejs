<%- include('header')%>

            <!-- PAGE CONTAINER-->
            <div class="content-page">
                <div class="content content-main">
                    <div class="loadings hidden"></div>
                    <!-- BEGIN PlACE PAGE CONTENT HERE -->
                    <!-- start page title -->
<div class="row ">
  <div class="col-xl-12">
    <div class="card">
      <div class="card-body">
        <h4 class="page-title">Dashboard </h4>
      </div> <!-- end card body-->
    </div> <!-- end card -->
  </div><!-- end col-->
</div>
<!-- end page title -->
<div class="row">
    <div class="col-12 event_calendar_content">
        <div class="row">
	<div class="col-md-6">
		<div class="card">
			<div class="card-body">
              <div class="items">
                <table id="basic-datatable" class="table table-striped dt-responsive nowrap" width="100%">
                  <thead>
                    <tr style="background-color: #313a46; color: #ababab;">
                      <th>Notices</th>
                    
                    </tr>
                  </thead>
                  <tbody>
                    <% if (notice){
                      for(var i=0; i < notice.length; i++){ %>
                      <tr>
                        <td>
                          <%= notice[i].noticedate %><br><br>Subject: <%= notice[i].subjectname %><br><%= notice[i].description %>
                        </td>
                      </tr>
                     <% }}
                     else {%>
                      <tr>
                        <td>no notice</td>
                      </tr>
                    <% }%>  
                  </tbody>
                </table>
              </div>      
			</div>
		</div>
	</div>
	<div class="col-md-6">
		<div class="card">
			<div class="card-body">
				<div class="items">
					<table id="basic-datatable" class="table table-striped dt-responsive nowrap" width="100%">
					  <thead>
						<tr style="background-color: #313a46; color: #ababab;">
							<th>Event Title</th>
							<th>From</th>
							<th>To</th>
						</tr>
					  </thead>
					<tbody>
						<% if (event){
              for(var i=0; i < event.length; i++){ %>
						<tr>
							<td><%= event[i].title %></td>
							<td><%= event[i].starting %></td>
							<td><%= event[i].ending %></td>
						</tr>
						<% } %>
            <% } %> 
					</tbody>
				    </table>
			    </div>
			</div>
		</div>
	</div>
</div>
</div>
</div>
</div>
</div>
</div>
<script type="text/javascript">
 
      function toggle_visibility(id) {
         var e = document.getElementById(id);
         if(e.style.display == 'block')
            e.style.display = 'none';
         else
            e.style.display = 'block';
      }

</script>
<script>
$('#myModal').on('shown.bs.modal', function () {
  $('#myInput').focus()
})
</script>   
    <%- include('footer')%>
