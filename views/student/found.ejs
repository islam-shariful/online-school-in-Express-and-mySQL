<%- include('header')%>
            <!-- PAGE CONTAINER-->
            <div class="content-page">
                <div class="content content-main">
                    <div class="loadings hidden"></div>
                    <!-- BEGIN PlACE PAGE CONTENT HERE -->
                    <!--title-->
<div class="row ">
  <div class="col-xl-12">
    <div class="card">
      <div class="card-body">
        <h4 class="page-title"> Lost & Found</h4>
      </div> <!-- end card body-->
    </div> <!-- end card -->
  </div><!-- end col-->
</div>


  <div class="row">
    <div class="col-12">
        <div class="card">
            <div class="card-body teacher_content">
                <table id="basic-datatable" class="table table-striped dt-responsive nowrap" width="100%">
                    <thead>
                        <tr style="background-color: #313a46; color: #ababab;">
                            <th>Item</th>
                            <th>Description</th>
                            <th>Lost Date</th>
                            <th>Found</th>
                            <th>Received</th>
                        </tr>
                    </thead>
                    <tbody>
                        <% if (found) { 
                            for(var i=0; i < found.length; i++){ %>
                        <tr>
                            <td><%= found[i].lostname %></td>
                            <td><%= found[i].lostdescription %></td>
                            <td><%= found[i].lostday %></td>
                            <td><%= found[i].found %></td>
                            <td><%= found[i].received %></td>
                        </tr>
                        <% } %>
                    <% }%>
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>
            </div>
        </div>
    </div>
</div>

<%- include('footer')%>